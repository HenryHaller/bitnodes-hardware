datadir={{ datadir }}
dbcache=64

par=2
checkblocks=24
checklevel=0
disablewallet=1

listen=1
bind=0.0.0.0
port={{ port }}
maxconnections={{ maxconnections }}
minrelaytxfee=0.0001

logips=1
printtoconsole=1

rpcbind={{ rpcbind }}
rpcallowip={{ rpcallowip }}
rpcport={{ rpcport }}
rpcuser={{ rpcuser }}
rpcpassword={{ rpcpassword }}
